<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".MainActivity" android:id="@+id/lt_tlo">
	<ImageView
			android:layout_width="30dp"
			android:layout_height="30dp" app:srcCompat="@drawable/ic_launcher_background" android:id="@+id/img_snake"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
	/>
	<TextView
			android:text="TextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/txt_sensor" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="2dp" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="2dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>